// WAP  t scan  5 string  from the user and display  them.

#include<stdio.h>
void main()
{
char s[5][10];
int i,ele;
ele=sizeof(s)/sizeof(s[0]);
printf("Enter the 5 string\n");
for(i=0;i<ele;i++)
scanf("%s",s[i]);
for(i=0;i<ele;i++)
printf("%s\n",s[i]);
}

// To ccopy  one string to another while swapping.

#include<stdio.h>
#include<string.h>
void main()
{
char s[5][[10],t[10];
int i,ele,j;
ele=sizeof(s)/sizeof(s[0]);
printf("Enter the 5string\n");
for(i=0;i<ele;i++)
scanf("%s",s[i]);
printf("Before sort...\n");
for(i=0;i<ele;i++)
printf("%s\n",s[i]);
for(i=0;i<ele-1;i++)
{
   for(j=0;j<ele-1-i;j++)
   {
   if(strcmp(s[j],,s[[j+1]>0)
   
   {
   strcpy(t,s[j]);
   strcpy(s[j],s[j+1]);
   strcpy(s[j+1],t);
   }
   }
   printf("After sort...\n");
   for(i=0;i<ele;i++)
   printf("%s\n",s[i]);
   }
  
  
 //VIRTUAL SORTING.
 
 #include<stdio.h>
 #include<string.h>
 void main()
 {
 char s[5][10],*t;
 char *p[5];
 int i,ele,j;
 ele=sizeof(s)/sizeof(s[0]);
 for(i=0;i<ele;i++)
 p[i]=s[i]
    
    printf("Enter the 5 string\n");
    for(i=0;i<ele;i++)
    scanf("%s",s[i]);
    printf("Before  sort...Array\n");
    for(i=0;i<ele;i++)
    printf("%s\n",s[i]);
  printf("Before sort...pointer\n");
  for(i=0;i<ele;i++)
  printf("%s\n",p[i]
        
        for(i=0;i<ele-1;i++)
        {
        for(j=0;j<ele-1-i;j++)
        {
        if(strcmp(p[j],p[j+1]>0)
        {
        t=p[j];
        p[j]=p[j+1];
        p[j+1]=t;
        }
        }
        }
   printf("After sort....Array\n");
   for(i=0;i<ele;i++)
   printf("%s\n",s[i});
   printf("After sort....pointer\n");
   for(i=0;i<ele;i++)
   printf("%s\n",p[i]);
   }
   
   
   //Design  a function  to print the elements of int  2D array.
   #include<stdio.h>
   void print(int,int,int(*)[]);
   void main()
   {
   int b[2][3]={{10,20,30},{40,50,60}};
   print(2,3,b);
   }
   void  print(int r,int c,int (*p)[c])
   {
   int i,j;
   for(i=0;i<r;i++)
   {
   for(j=0;j<c;j++)
   printf("%d ",p[i][j]);
   printf("\n");
   }
   
